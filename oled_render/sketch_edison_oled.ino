void OLED_WrDat(unsigned char dat)//写数据
{
	unsigned char i;
  digitalWrite(OLED_SPI_CS, 0);
	OLED_DC_Set();
#ifdef USE_EMU_SPI
	for(i=0;i<8;i++)
	{
		if((dat << i) & 0x80)
		{
			OLED_SDA_Set();
		}
		else
			OLED_SDA_Clr();
		OLED_SCL_Clr();
		OLED_SCL_Set();
      
	}
#else

  SPI.transfer(dat);
 
#endif
 digitalWrite(OLED_SPI_CS, 1);
}

void OLED_WrCmd(unsigned char cmd)//写命令
{
	unsigned char i;
 digitalWrite(OLED_SPI_CS, 0);
	OLED_DC_Clr();
          delay(1);
#ifdef USE_EMU_SPI
	for(i=0;i<8;i++) //发送一个八位数据
	{
		if((cmd << i) & 0x80)
		{
			OLED_SDA_Set();
		}
		else
		{
			OLED_SDA_Clr();
		}
		OLED_SCL_Clr();
		OLED_SCL_Set();
	}
#else

  SPI.transfer(cmd);

#endif
  digitalWrite(OLED_SPI_CS, 1);
}

void OLED_SetPos(unsigned char x, unsigned char y)//设置起始点坐标
{
	OLED_WrCmd(0xb0 + y);
	OLED_WrCmd(((x&0xf0)>>4)|0x10);
	OLED_WrCmd((x&0x0f));
}

void OLED_Fill(unsigned char bmp_dat)//全屏填充
{
	unsigned char y,x;
	for(y=0;y<8;y++)
	{
		OLED_WrCmd(0xb0+y);
		OLED_WrCmd(0x01);
		OLED_WrCmd(0x10);
		for(x=0;x<X_WIDTH;x++)
		{
			OLED_WrDat(bmp_dat);
		}
	}
}

void OLED_CLS(void)//清屏
{
	OLED_Fill(0x00);
}

